{
  "name": "@uic/annotator",
  "version": "0.1.0",
  "description": "Source code annotator that inserts data-agent-id attributes into UI components",
  "type": "module",
  "license": "MIT",
  "author": "UIC Contributors",
  "homepage": "https://github.com/sherifkozman/uicontract/tree/main/packages/annotator",
  "repository": {
    "type": "git",
    "url": "https://github.com/sherifkozman/uicontract.git",
    "directory": "packages/annotator"
  },
  "bugs": {
    "url": "https://github.com/sherifkozman/uicontract/issues"
  },
  "keywords": ["uic", "ui-contracts", "annotator", "data-agent-id", "source-transform"],
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist", "LICENSE", "README.md"],
  "scripts": {
    "build": "tsc -b",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src"
  },
  "dependencies": {
    "@uic/core": "workspace:*"
  },
  "devDependencies": {
    "@uic/parser-react": "workspace:*",
    "@uic/namer": "workspace:*"
  }
}
